<% @page_title = "新增團購" %>

<h2>新增團購</h2>
<h3><%= @store.name %></h3>

<%= simple_form_for [@store, @groupon] do |f| %>
  <%= f.input :deadline %> <br>
  <%= f.input :is_secret, label: "不公開", as: :boolean %> <br>
  <%= f.submit "Submit", class: "btn btn-default", data: { disable_with: "Submitting..." } %>
<% end %>
